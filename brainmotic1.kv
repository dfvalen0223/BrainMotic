<Button_enter@Button>:
    Image:
        source: './ENTER1.png'
        y: self.parent.y + self.parent.height - 110
        x: self.parent.x + self.parent.width - 300
        size: 204, 104
        allow_stretch: True

<ZeroScreen>:
    name:'START'

    canvas:
        Color:
            hsv:  .2, .8, .6
# .9, .4, .1  negro
        Rectangle:
            size: self.size
    BoxLayout:
        padding:20
        spacing:10
        orientation: 'vertical'
        Button:
            background_color: [.2,.7,.0,.1]
            on_release:  root.manager.current = root.manager.next()
            Image:
                source: './HAKA.png'
                y: self.parent.y + self.parent.height - 350
                x: self.parent.x + self.parent.width - 500
                size: 250, 250
                allow_stretch: True
        Button:
            size_hit_y: 10
            font_size: '50sp'
            text: root.name+"[color=3333ff][/color]"
            markup: True
            on_release:  root.manager.current = root.manager.next()
            background_color: [.2,.7,.0,.1]


# REMEMBER CLOSE THE APP app.Exit()

<FirstScreen>:
    name: 'Bedroom'
     #.6 #.2 #COLORES POSIBLES COMBINACIONES DE HSV(*,.8,.4)
    canvas:
        Color:
            hsv: .2, .8, .6
        Rectangle:
            size: self.size

    BoxLayout:
        orientation: 'vertical'
        BoxLayout:
            Label:
                font_size: '50sp'
                text: "Bedroom[/color]"
                markup: True
                pos:20, 100
                background_color: [.6, .8, 1, 2]

        BoxLayout:
            orientation: 'horizontal'
            size_hint_y:None
            height: sp(100)
            pading: 5  # alrededor del widget
            Button:
               # text:"NEXT"
                background_color: [.0, .5, .6, 8]
                on_release: app.root.current = 'Kitchen'
                Image:
                    source: './NEXT1.png'
                    y: self.parent.y + self.parent.height - 110
                    x: self.parent.x + self.parent.width - 300
                    size: 204, 104
                    allow_stretch: True

            Button_enter:
                text:"ENTER"
                background_color: [.0, .5, .6, 8]
                on_release:root.manager.current=root.manager.next()



